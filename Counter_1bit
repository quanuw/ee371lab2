#! /c/iverilog/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0060C118 .scope module, "Counter_1bit_testbench" "Counter_1bit_testbench" 2 4;
 .timescale 0 0;
v00AC3A90_0 .net "Clock", 0 0, v00AC4F40_0; 1 drivers
v00AC3AE8_0 .net "Count", 0 0, v00AC46B0_0; 1 drivers
v00AC4440_0 .net "Increase", 0 0, v0060A1C8_0; 1 drivers
v00AC4498_0 .net "Reset", 0 0, v0060AA28_0; 1 drivers
S_0060C228 .scope module, "t" "Counter_1bit_tester" 2 11, 3 1, S_0060C118;
 .timescale 0 0;
P_00AC2654 .param/l "CLOCK_PERIOD" 3 5, +C4<010>;
v00AC4F40_0 .var "Clock", 0 0;
v0060A1C8_0 .var "Increase", 0 0;
v0060AA28_0 .var "Reset", 0 0;
S_0060C1A0 .scope module, "dut" "Counter_1bit" 2 15, 4 1, S_0060C118;
 .timescale 0 0;
P_00AC10A4 .param/l "A" 4 12, +C4<0>;
P_00AC10B8 .param/l "B" 4 12, +C4<01>;
P_00AC10CC .param/l "C" 4 12, +C4<0>;
v0060E4D8_0 .alias "Clock", 0 0, v00AC3A90_0;
v0060E558_0 .alias "Count", 0 0, v00AC3AE8_0;
v00AC3A00_0 .alias "Increase", 0 0, v00AC4440_0;
v00AC43B0_0 .alias "Reset", 0 0, v00AC4498_0;
v00AC4530_0 .var "ns", 0 0;
v00AC46B0_0 .var "ps", 0 0;
E_00AC25D0 .event posedge, v0060E4D8_0;
E_00AC2630 .event edge, v00AC3A00_0, v00AC46B0_0;
    .scope S_0060C228;
T_0 ;
    %set/v v00AC4F40_0, 1, 1;
    %end;
    .thread T_0;
    .scope S_0060C228;
T_1 ;
    %delay 1, 0;
    %load/v 8, v00AC4F40_0, 1;
    %inv 8, 1;
    %set/v v00AC4F40_0, 8, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0060C228;
T_2 ;
    %wait E_00AC25D0;
    %ix/load 0, 1, 0;
    %assign/v0 v0060AA28_0, 0, 1;
    %wait E_00AC25D0;
    %wait E_00AC25D0;
    %ix/load 0, 1, 0;
    %assign/v0 v0060AA28_0, 0, 0;
    %ix/load 0, 1, 0;
    %assign/v0 v0060A1C8_0, 0, 1;
    %wait E_00AC25D0;
    %ix/load 0, 1, 0;
    %assign/v0 v0060A1C8_0, 0, 0;
    %wait E_00AC25D0;
    %wait E_00AC25D0;
    %ix/load 0, 1, 0;
    %assign/v0 v0060A1C8_0, 0, 1;
    %wait E_00AC25D0;
    %ix/load 0, 1, 0;
    %assign/v0 v0060A1C8_0, 0, 0;
    %wait E_00AC25D0;
    %wait E_00AC25D0;
    %ix/load 0, 1, 0;
    %assign/v0 v0060A1C8_0, 0, 1;
    %wait E_00AC25D0;
    %ix/load 0, 1, 0;
    %assign/v0 v0060A1C8_0, 0, 0;
    %wait E_00AC25D0;
    %wait E_00AC25D0;
    %ix/load 0, 1, 0;
    %assign/v0 v0060A1C8_0, 0, 1;
    %wait E_00AC25D0;
    %ix/load 0, 1, 0;
    %assign/v0 v0060A1C8_0, 0, 0;
    %wait E_00AC25D0;
    %wait E_00AC25D0;
    %ix/load 0, 1, 0;
    %assign/v0 v0060A1C8_0, 0, 1;
    %wait E_00AC25D0;
    %ix/load 0, 1, 0;
    %assign/v0 v0060A1C8_0, 0, 0;
    %wait E_00AC25D0;
    %wait E_00AC25D0;
    %end;
    .thread T_2;
    .scope S_0060C1A0;
T_3 ;
    %set/v v00AC46B0_0, 0, 1;
    %end;
    .thread T_3;
    .scope S_0060C1A0;
T_4 ;
    %wait E_00AC2630;
    %load/v 8, v00AC3A00_0, 1;
    %cmpi/u 8, 1, 1;
    %jmp/1 T_4.0, 6;
    %cmpi/u 8, 0, 1;
    %jmp/1 T_4.1, 6;
    %jmp T_4.2;
T_4.0 ;
    %load/v 8, v00AC46B0_0, 1;
    %mov 9, 0, 31;
    %addi 8, 1, 32;
    %set/v v00AC4530_0, 8, 1;
    %jmp T_4.2;
T_4.1 ;
    %load/v 8, v00AC46B0_0, 1;
    %set/v v00AC4530_0, 8, 1;
    %jmp T_4.2;
T_4.2 ;
    %jmp T_4;
    .thread T_4, $push;
    .scope S_0060C1A0;
T_5 ;
    %wait E_00AC25D0;
    %load/v 8, v00AC43B0_0, 1;
    %jmp/0xz  T_5.0, 8;
    %ix/load 0, 1, 0;
    %assign/v0 v00AC46B0_0, 0, 0;
    %jmp T_5.1;
T_5.0 ;
    %load/v 8, v00AC4530_0, 1;
    %ix/load 0, 1, 0;
    %assign/v0 v00AC46B0_0, 0, 8;
T_5.1 ;
    %jmp T_5;
    .thread T_5;
    .scope S_0060C118;
T_6 ;
    %vpi_call 2 19 "$dumpfile", "Counter_1bit.vcd";
    %vpi_call 2 20 "$dumpvars", 1'sb0, S_0060C118;
    %delay 150, 0;
    %vpi_call 2 22 "$finish";
    %end;
    .thread T_6;
